<?xml version="1.0" encoding="UTF-8" ?>
<suite name="Google Test" parallel="tests" thread-count="3">
    <test name="Firefox Test">
        <parameter name="browser" value="Firefox"/>
        <classes>
            <class name="GoogleTest">
                <methods>
                    <include name="verifyTitle"/>
                    <include name="verifySearchButton"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Chrome Test">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="GoogleTest">
            </class>
        </classes>
    </test>
</suite>